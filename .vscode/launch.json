{
    "version": "0.2.0",
    "configurations": [
        
        
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Debug ros_acados_nodegen.generator (module)",
            "type": "debugpy",
            "request": "launch",
            "module": "ros_acados_nodegen.generator",
            "args": [
                "${workspaceFolder}/examples/scripts/c_generated_code/safety_filter_ocp.json",
                "${workspaceFolder}/ros_acados_nodegen/config/all_configs.yaml",
                // "--set", "acados.model.name=sf_mpc",
                "--set",
                "script_path=${workspaceFolder}/examples/scripts/safety_filter_mpc.py",
                "--set",
                "package.name=mpc_pkg",
                "--set",
                "package.license=Apache-2.0",
            ],
            "python": "${command:python.interpreterPath}",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}